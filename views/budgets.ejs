<%- include('headers/header.System.ejs')%>

<body>
    <h1>Presupuestos</h1>
    <br>
    <br>
    <br>
    <button>nuevo presupuesto</button>
        <table border="1">
            <tr>
                <th>Id </th>
                <th>proyecto</th>
                <th>version</th>
                <th>Fecha</th>
              </tr>
            <% for(var i=0; i < resulBudgets.length; i++) { %>
               <tr>
                <td><%= resulBudgets[i].id_budget %></td>
                <td><%= resulBudgets[i].proyect %></td>
                <td><%= resulBudgets[i].version %></td>
                <td><%= resulBudgets[i].createdAt %></td>
                <td><button id='<%= resulBudgets[i].id_budget %>'>Editar</button></td>
                 <td><button id='<%= resulBudgets[i].id_budget %>'>Eliminar</button></td>
                 <td><button id='<%= resulBudgets[i].id_budget %>'>Enviar</button></td>
               </tr>
            <% } %>
        </table>            
<br>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</body>
</html>